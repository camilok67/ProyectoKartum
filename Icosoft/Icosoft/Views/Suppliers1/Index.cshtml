@model IEnumerable<Icosoft.Models.Supplier>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.City.CityName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SuplierName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.City.CityName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SuplierName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idSuplier }) |
            @Html.ActionLink("Details", "Details", new { id=item.idSuplier }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.idSuplier })
        </td>
    </tr>
}

</table>
